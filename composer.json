{
	"name": "kdyby/doctrine-cache",
	"type": "library",
	"description": "Doctrine Cache bridge for Nette Framework",
	"keywords": ["nette", "kdyby", "doctrine", "cache"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/doctrinecache/issues"
	},
	"require": {
		"php": "^7.1",
		"doctrine/cache": "^1.8",
		"nette/di": "^3.0",
		"nette/caching": "^3.0"
	},
	"require-dev": {
		"doctrine/orm": "^2.6",
		"symfony/validator": "^2.5.3 || ^3.0 || ^4.0",
		"nette/bootstrap": "^3.0",
		"nette/tester": "^2.2",
		"kdyby/coding-standard": "^2.0@dev",
		"phpstan/phpstan-shim": "^0.11",
		"jakub-onderka/php-parallel-lint": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Kdyby\\DoctrineCache\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\": "tests/KdybyTests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.7-dev"
		},
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"src/DI/ClassAliasMap.php"
			]
		}
	}
}
